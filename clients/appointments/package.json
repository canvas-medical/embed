{
  "name": "@canvas-medical/embed-appointments",
  "version": "0.0.6",
  "private": false,
  "scripts": {
    "build": "webpack build",
    "start": "webpack server --open",
    "test": "jest ./tests"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h",
          "pragmaFrag": "Fragment"
        }
      ],
      "babel-plugin-styled-components"
    ],
    "presets": [
      "@babel/env",
      [
        "@babel/typescript",
        {
          "jsxPragma": "h"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "preact",
      "prettier"
    ]
  },
  "jest": {
    "preset": "jest-preset-preact",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 10,
        "lines": 25,
        "statements": 25
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@canvas-medical/embed-common": "^0.0.6",
    "preact": "^10.6.5",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@nick.telsan/eslint-config-preact": "^1.3.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/preact": "^2.0.1",
    "babel-plugin-styled-components": "^2.0.2",
    "babel-preset-preact": "^2.0.0",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-preset-preact": "^4.0.5",
    "preact-render-to-string": "^5.1.19",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
